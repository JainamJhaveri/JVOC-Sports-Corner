<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rishabhjain.jvocsportscorner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEvent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddSubEvent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SEParticipants"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddParticipants"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ViewPSE"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ViewPSEP"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ViewPEAllP"
            android:screenOrientation="portrait" />
        <activity android:name=".MatchScheduleActivity"></activity>
    </application>

</manifest>